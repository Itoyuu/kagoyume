<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>seach</servlet-name>
        <servlet-class>kago.serch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ItemDetail</servlet-name>
        <servlet-class>kago.ItemDetail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add</servlet-name>
        <servlet-class>kago.add</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cart</servlet-name>
        <servlet-class>kago.cart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuyConfirm</servlet-name>
        <servlet-class>kago.BuyConfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BuyComplete</servlet-name>
        <servlet-class>kago.BuyComplete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>kago.registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrationconfirm</servlet-name>
        <servlet-class>kago.registrationconfirm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registrationcomplete</servlet-name>
        <servlet-class>kago.registrationcomplete</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserDataDAO</servlet-name>
        <servlet-class>DB.UserDataDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>kago.login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>seach</servlet-name>
        <url-pattern>/seach</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ItemDetail</servlet-name>
        <url-pattern>/ItemDetail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add</servlet-name>
        <url-pattern>/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cart</servlet-name>
        <url-pattern>/cart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuyConfirm</servlet-name>
        <url-pattern>/BuyConfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuyComplete</servlet-name>
        <url-pattern>/BuyComplete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrationconfirm</servlet-name>
        <url-pattern>/registrationconfirm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registrationcomplete</servlet-name>
        <url-pattern>/registrationcomplete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserDataDAO</servlet-name>
        <url-pattern>/UserDataDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
